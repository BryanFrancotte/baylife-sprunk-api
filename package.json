    "@prisma/extension-accelerate": "^3.0.0",
    "dotenv": "^17.2.3",
    "elysia": "latest",
    "pg": "^8.16.3",
    "prismabox": "^1.1.25"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "bun-types": "latest",
    "prisma": "^7.0.1"
